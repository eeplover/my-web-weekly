<table border="0" width="100%" cellpadding="0">
  <tbody>
    <tr>
      <td align="center">
        <table id="github-trending" border="0" width="600" cellpadding="0" cellspacing="0">
          <thead>
            <tr>
              <th align="left">Github Trending Js-Top-5</th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <td>
                <% githubTrending.forEach(item => { %>
                <a href="<%= item.link %>"><%= item.title %></a>
                <p><%- item.desc %></p>
                <p><%- item.stars %></p>
                <br clear="all" style="clear: both">
                <% }) %>
              </td>
            </tr>
          </tbody>
        </table>
      </td>
    </tr>
  </tbody>
</table>
