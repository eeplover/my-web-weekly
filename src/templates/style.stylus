/* General Style */
body
  background-color white
a
  color #0081b5 !important

#other,
#front-end-focus,
#node-weekly,
#github-trending,
#js-weekly
  thead
    color white
    th
      padding 6px 12px
      font-size 23px
  tbody
    td
      padding 16px 12px 12px 12px
    td > a
      font-size 18px
    p
      margin 0
      color #586069
      & > a
        font-size 14px

/* Github Trending Js-Top-5 Style */
#github-trending
  thead
    background-color #24292e
  tbody
    p
      svg
        vertical-align sub

/* JavaScript Weekly */
#js-weekly
  thead
    color #323330
    background-color #f7df1e

/* FrontEnd Focus */
#front-end-focus
  thead
    background-color #9d3979

/* Node Weekly */
#node-weekly
  thead
    background-color #6ca629

/* Other */
#other
  thead
    background-color #ac988e
